@model List<TestUygulama2.DataDB.User>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <table style="width:100%">
        <thead>
            <tr>
                <td>Id</td>
                <td>İsim</td>
                <td>Soyisim</td>
                <td>Yaş</td>
            </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Surname</td>
                <td>@item.Age</td>
            </tr>
        }
        </tbody>
    </table>
</div>
